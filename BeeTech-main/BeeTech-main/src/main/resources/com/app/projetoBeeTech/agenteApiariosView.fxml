<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="636.0" prefWidth="895.0" styleClass="login-form" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.projetoBeeTech.controller.agente.AgenteApiariosController">
    <stylesheets>
        <URL value="@design.css" />
    </stylesheets>
    <children>
        <ComboBox fx:id="comboBoxSelecionarApicultor" layoutX="30.0" layoutY="29.0" prefHeight="24.0" prefWidth="458.0" promptText="Selecione o Apicultor" styleClass="textfield" />
        <TableView fx:id="tabelaApicultorApiario" layoutX="28.0" layoutY="68.0" prefHeight="363.0" prefWidth="842.0">
            <columns>
                <TableColumn fx:id="colunaNomeApicultor" prefWidth="248.33334350585938" text="Apicultor" />
                <TableColumn fx:id="colunaNomeApiario" prefWidth="256.9999694824219" text="Apiario" />
                <TableColumn fx:id="colunaLocalizacaoApiario" prefWidth="335.0" text="Localizaçao do apiario" />
            </columns>
        </TableView>
        <Button fx:id="buttonAdicionarApiario" layoutX="497.0" layoutY="469.0" mnemonicParsing="false" onAction="#adicionarApiario" text="Adicionar" styleClass="login-btn" />
        <TextField fx:id="textFieldNomeApiario" layoutX="65.0" layoutY="455.0" promptText="Nome do apiario" styleClass="textfield" />
        <TextArea fx:id="textAreaLocalizacaoApiario" layoutX="66.0" layoutY="510.0" prefHeight="74.0" prefWidth="299.0" promptText="Localizaçao do apiario" styleClass="textfield" />
        <Button fx:id="buttonAtualizarApiario" layoutX="636.0" layoutY="469.0" mnemonicParsing="false" onAction="#atualizarApiario" text="Atualizar" styleClass="login-btn" />
        <Button fx:id="buttonExcluirApiario" layoutX="575.0" layoutY="519.0" mnemonicParsing="false" onAction="#excluirApiario" text="Excluir" styleClass="cancel-btn" />
    </children>
</AnchorPane>
