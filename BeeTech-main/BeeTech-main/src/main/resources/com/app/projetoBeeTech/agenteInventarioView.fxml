                <TableColumn fx:id="colunaBensQuantidade" prefWidth="99.00001525878906" text="Quantidade" />
                <TableColumn fx:id="colunaBensValor" prefWidth="77.33331298828125" text="Valor" />
                <TableColumn fx:id="colunaBensData" prefWidth="110.33334350585938" text="Data" />
                <TableColumn fx:id="colunaBensDescricao" prefWidth="308.0000305175781" text="Descriçao" />
