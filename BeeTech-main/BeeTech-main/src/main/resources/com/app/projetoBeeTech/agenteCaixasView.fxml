<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="636.0" prefWidth="895.0" styleClass="login-form" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.projetoBeeTech.controller.agente.AgenteCaixasController">
    <stylesheets>
        <URL value="@design.css" />
    </stylesheets>
    <children>
        <ComboBox fx:id="comboBoxApicultor" layoutX="39.0" layoutY="32.0" prefHeight="24.0" prefWidth="412.0" promptText="Selecione o Apicultor" styleClass="textfield" />
        <ComboBox fx:id="comboBoxApiario" layoutX="480.0" layoutY="32.0" prefHeight="24.0" prefWidth="377.0" promptText="Selecione o Apiario" styleClass="textfield" />
        <TableView fx:id="tabelaCaixas" layoutX="33.0" layoutY="82.0" prefHeight="310.0" prefWidth="834.0">
            <columns>
                <TableColumn fx:id="colunaNomeApiario" prefWidth="402.66668701171875" text="Apiario" />
                <TableColumn fx:id="colunaIdentificacaoCaixa" prefWidth="430.33331298828125" text="Identificaçao da caixa" />
            </columns>
        </TableView>
        <TextField fx:id="textFieldIdentificacaoCaixa" layoutX="68.0" layoutY="439.0" prefHeight="24.0" prefWidth="212.0" promptText="Identificaçao da caixa" styleClass="textfield" />
        <Button fx:id="buttonAdicionarCaixa" layoutX="389.0" layoutY="439.0" mnemonicParsing="false" onAction="#adicionarCaixa" text="Adicionar" styleClass="login-btn" />
        <Button fx:id="buttonAtualizarCaixa" layoutX="527.0" layoutY="439.0" mnemonicParsing="false" onAction="#atualizarCaixa" text="Atualizar" styleClass="login-btn" />
        <Button fx:id="buttonExcluirCaixa" layoutX="480.0" layoutY="496.0" mnemonicParsing="false" onAction="#excluirCaixa" text="Excluir" styleClass="cancel-btn" />
    </children>
</AnchorPane>
