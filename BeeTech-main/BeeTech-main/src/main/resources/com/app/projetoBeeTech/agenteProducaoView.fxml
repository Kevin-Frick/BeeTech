<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="636.0" prefWidth="895.0" styleClass="login-form" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.projetoBeeTech.controller.agente.AgenteProducaoController">
    <stylesheets>
        <URL value="@design.css" />
    </stylesheets>
    <children>
        <ComboBox fx:id="comboBoxApicultor" layoutX="39.0" layoutY="36.0" prefHeight="24.0" prefWidth="375.0" promptText="Selecione o apicultor" styleClass="textfield" />
        <ComboBox fx:id="comboBoxApiario" layoutX="473.0" layoutY="36.0" prefHeight="24.0" prefWidth="327.0" promptText="Selecione o apiario" styleClass="textfield" />
        <ComboBox fx:id="comboBoxCaixa" layoutX="329.0" layoutY="89.0" prefHeight="24.0" prefWidth="234.0" promptText="Selecione a caixa" styleClass="textfield" />
        <TableView fx:id="tabelaProducao" layoutX="39.0" layoutY="135.0" prefHeight="275.0" prefWidth="821.0">
            <columns>
                <TableColumn fx:id="colunaTipoProducao" prefWidth="276.3333206176758" text="Tipo" />
                <TableColumn fx:id="colunaQuantidadeProducao" prefWidth="291.6666793823242" text="Quantidade" />
                <TableColumn fx:id="colunaDataProducao" prefWidth="252.66665649414062" text="Data" />
            </columns>
        </TableView>
        <ComboBox fx:id="comboBoxTipoProducao" layoutX="55.0" layoutY="424.0" prefHeight="24.0" prefWidth="245.0" promptText="Selecione o tipo da produçao" styleClass="textfield" />
        <TextField fx:id="textFieldQuantidadeProducao" layoutX="54.0" layoutY="482.0" prefHeight="24.0" prefWidth="245.0" promptText="Insira a quantidade desta produçao" styleClass="textfield" />
        <DatePicker fx:id="DatePickerDataProducao" layoutX="54.0" layoutY="538.0" prefHeight="24.0" prefWidth="176.0" promptText="Data" styleClass="textfield" />
        <Button fx:id="buttonAdicionarProducao" layoutX="456.0" layoutY="447.0" mnemonicParsing="false" onAction="#adicionarProducao" text="Adicionar" styleClass="login-btn" />
        <Button fx:id="buttonAtualizarProducao" layoutX="605.0" layoutY="447.0" mnemonicParsing="false" onAction="#atualizarProducao" text="Atualizar" styleClass="login-btn" />
        <Button fx:id="buttonExcluirProducao" layoutX="531.0" layoutY="505.0" mnemonicParsing="false" onAction="#excluirProducao" text="Excluir" styleClass="cancel-btn" />
